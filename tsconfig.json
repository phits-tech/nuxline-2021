{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "..",
    "outDir": "dist",
    "paths": {
      "@/*": ["src/*"]
    },
    // Override
    "composite": false,
    "importHelpers": true,
    "incremental": false,
    "jsx": "preserve",
    "target": "ESNext",
    "module": "ESNext"
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue"]
}
